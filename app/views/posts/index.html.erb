<h1>Disaster Information in Makati [Forum]</h1>
<table class="table">
  <thead>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Address</th>
    <th>User</th>
    <th>Action</th>
  </tr>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.address %></td>
      <td><%= post.user.email %></td>
    </tr>
  <% end %>
</table>

<%= link_to "New", new_post_path %>